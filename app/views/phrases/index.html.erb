<div class="container-fluid">
<div>
	<p class="mt-5"></p>
</div>

<% if user_signed_in? %>
	<div class="row">
	  <div class="col-sm-3">	
		<%= render 'profile' %>
	  </div>

	  <div class="col-sm-6">	
		<%= render 'feed' %>
	  </div>

	  <div class="col-sm-3">	
		<%= render 'trends' %>
	  </div>
	</div>
<% else %>
	<div class="row">
	  <div class="col-sm-8">	
		<%= render 'feed' %>
	  </div>

	  <div class="col-sm-4">	
		<%= render 'trends' %>
	  </div>
	</div>
<% end %>
</div>